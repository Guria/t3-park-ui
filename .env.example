# This file is committed to version control. Do not add secrets here.
# Create a copy named ".env" and populate it with your actual values.

# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/db"

# Next Auth
# Generate a secret: openssl rand -base64 32
AUTH_SECRET=""

# Base URL
# Optional: Will default to localhost:3000 on server, or window.location.origin on client
# BASE_URL=""

# Logging
# Optional: Defaults to true in development, false in production
# TRPC_LOGGER_LINK_ENABLED=

# Prisma Logging
# Optional: Comma-separated list of log levels (info, query, warn, error)
# Defaults to "query,error,warn" in development, "error" in production
# PRISMA_LOG_LEVEL=

# Credentials Provider
# Optional: Defaults to true in development, false in production
# CREDENTIALS_PROVIDER_ENABLED=

# Default email for Credentials Provider
# Optional: Defaults to test@example.com
# CREDENTIALS_PROVIDER_DEFAULT_EMAIL=

# Env Validation
# Optional: Set to any value to skip validation
# SKIP_ENV_VALIDATION=
